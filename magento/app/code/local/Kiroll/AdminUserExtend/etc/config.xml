<?xml version="1.0"?>

<config>
    <modules>
        <Kiroll_AdminUserExtend>
            <version>0.1.0</version>
        </Kiroll_AdminUserExtend>
    </modules>
    <global>
        <blocks>
            <admin_user_extend>
                <class>Kiroll_AdminUserExtend_Block</class>
            </admin_user_extend>
            <adminhtml>
                <rewrite>
                    <system_account_edit_form>Kiroll_AdminUserExtend_Block_Adminhtml_System_Account_Edit_Form</system_account_edit_form>
                    <permissions_user_edit_tab_main>Kiroll_AdminUserExtend_Block_Adminhtml_Permissions_User_Edit_Tab_Main</permissions_user_edit_tab_main>
                </rewrite>
            </adminhtml>
        </blocks>
        <models>
            <admin_user_extend>
                <class>Kiroll_AdminUserExtend_Model</class>
            </admin_user_extend>
        </models>
        <helpers>
            <admin_user_extend>
                <class>Kiroll_AdminUserExtend_Helper</class>
            </admin_user_extend>
        </helpers>
        <resources>
            <admin_user_extend_setup>
                <setup>
                    <module>Kiroll_AdminUserExtend</module>
                </setup>
            </admin_user_extend_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <admin_user_extend before="Mage_Adminhtml">Kiroll_AdminUserExtend_Adminhtml</admin_user_extend>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>